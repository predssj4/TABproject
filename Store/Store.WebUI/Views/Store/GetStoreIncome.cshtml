@model IEnumerable<Store.DataAccess.ViewModels.StoreIncomeViewModel>

@{
    ViewBag.Title = "GetStoreIncome";
}

<h2>GetStoreIncome</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StoreId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Income)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StoreId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Income)
        </td>
        <td>
            @Html.ActionLink("Details", "IncomeDetails", new {  id=item.StoreId })
        </td>
        <td>
            @Html.ActionLink("Add Store Houses", "AddStoreHouse", new { id = item.StoreId })
        </td>
    </tr>
}

</table>
